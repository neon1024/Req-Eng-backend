{
  "name": "req-eng-backend",
  "version": "1.0.0",
  "description": "Express API with JWT authentication",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "test": "node -r dotenv/config node_modules/@cucumber/cucumber/bin/cucumber-js --require-module ts-node/register --require features/step-definitions/**/*.ts --require features/support/**/*.ts features/**/*.feature"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^5.2.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "chai": "^6.2.2",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
